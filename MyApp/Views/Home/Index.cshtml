@model IEnumerable<Employee>
@{ 
    ViewBag.Title= "Employee List";
}
    <div class="card-deck">
        @foreach(var emp in Model)
        {
        <div class="card m-3">
            <div class="card-header">
                <h3>@emp.FullName</h3>
            </div>
            <img class="card-img-top" src="~/images/labrador.jpg"/>
            <div class="card-footer text-center">
                <a href="#" class="btn btn-primaty">View</a>
                <a href="#" class="btn btn-primaty">Edit</a>
                <a href="#" class="btn btn-danger">Delete</a>
            </div>
        </div>
        }
    </div>
